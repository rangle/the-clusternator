{
  "name": "clusternator",
  "version": "0.1.14",
  "description": "Program to deploy docker containers on cloud service platforms with minimal user input, and/or configuration",
  "main": "index.js",
  "preferGlobal": true,
  "keywords": [
    "docker",
    "aws",
    "amazon",
    "cluster",
    "cloud",
    "heroku",
    "vm",
    "virtual machine",
    "container"
  ],
  "bugs": {
    "url": "https://github.com/rangle/the-clusternator/issues"
  },
  "os": [
    "!win32"
  ],
  "scripts": {
    "postinstall": "npm run build",
    "start": "node ./server-init.js",
    "staging": "NODE_ENV=staging; node ./server-init.js",
    "dev": "NODE_ENV=debug; nodemon ./server-init.js",
    "test": "gulp test-unit",
    "build": "gulp transpile",
    "prepublish": "npm run build",
    "graph": "echo 'Requires GraphViz (If it is not already installed)'; madge --exclude \"node_modules|bin-src|lib|path|spec|coverage|tmp|examples\"  --image coverage/clusternator-graph.png ./",
    "docker-build": "npm run build && docker build -t rafkhan/clusternator-test ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/the-clusternator"
  },
  "author": "rangle.io",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.2.12",
    "base64url": "^1.0.5",
    "body-parser": "^1.14.2",
    "body-parser-rawbody": "^1.13.2",
    "compression": "^1.6.0",
    "connect-ensure-login": "^0.1.1",
    "credential": "^0.2.6",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "express-winston": "^0.4.1",
    "inquirer": "^0.11.0",
    "kbpgp": "^2.0.48",
    "letsencrypt-express": "^1.0.3",
    "localhost.daplie.com-certificates": "^1.1.2",
    "media-typer": "^0.3.0",
    "mkdirp": "^0.5.1",
    "parse-git-config": "^0.3.1",
    "passport": "^0.3.2",
    "passport-http-header-token": "^1.0.0",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "ramda": "^0.17.1",
    "raw-body": "^2.1.4",
    "request": "^2.67.0",
    "request-promise": "^0.4.3",
    "rimraf": "^2.4.3",
    "semver": "^5.1.0",
    "superagent": "^1.4.0",
    "winston": "^1.1.2",
    "yargs": "^3.29.0"
  },
  "bin": {
    "clusternator": "./bin/clusternator.sh"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "eslint": "^1.8.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "gulp-eslint": "^1.0.0",
    "gulp-istanbul": "^0.10.2",
    "gulp-mocha": "^2.1.3",
    "madge": "^0.5.3",
    "mocha": "^2.3.3",
    "mock-fs": "^3.6.0",
    "mock-spawn": "^0.2.6",
    "nodemon": "^1.8.1",
    "rewire": "^2.3.4",
    "supertest": "^1.1.0"
  }
}
